class Solution:
    def combinationSum(self, candidates: List[int], target: int) -> List[List[int]]:
        ans = []
        candidates.sort()
        def helper(ind,target,row):
            if target == 0 :
                ans.append(row[:])
                return
            if ind < len(candidates) and candidates[ind] <= target:
                for i in range(ind,len(candidates)):
                    row.append(candidates[ind])
                    helper(i+1,target-candidates[ind],row)
                    row.pop()

        helper(0,target,[])
        return ans
